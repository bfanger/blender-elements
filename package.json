{
  "name": "blender-elements",
  "version": "1.0.0",
  "description": "Bringing the UI controls from Blender to the web",
  "main": "dist/index.js",
  "scripts": {
    "storybook": "start-storybook -s ./build -p 6006",
    "build-storybook": "build-storybook",
    "test": "eslint src stories",
    "build": "webpack --mode production",
    "lint": "eslint --ext .js --max-warnings 0 --ignore-path .gitignore ."
  },
  "author": "Bob Fanger <bfanger@gmail.com>",
  "license": "GPL-3.0",
  "eslintConfig": {
    "root": true,
    "env": {
      "browser": true,
      "jest": true
    },
    "parserOptions": {
      "parser": "babel-eslint"
    },
    "extends": [
      "esnext",
      "plugin:react/recommended",
      "plugin:prettier/recommended"
    ],
    "plugins": [
      "only-warn"
    ],
    "globals": {
      "blenderElements": true
    },
    "settings": {
      "react": {
        "version": "16"
      }
    }
  },
  "devDependencies": {
    "@storybook/addon-actions": "^4.1.7",
    "@storybook/addon-knobs": "^4.1.7",
    "@storybook/addon-links": "^4.1.7",
    "@storybook/addon-notes": "^4.1.7",
    "@storybook/react": "^4.1.7",
    "babel-core": "^6.26.3",
    "babel-eslint": "^10.0.1",
    "bootstrap": "^4.1.2",
    "brace": "^0.11.1",
    "css-loader": "^2.1.0",
    "dejavu-sans": "^1.0.0",
    "eslint": "^5.6.0",
    "eslint-config-esnext": "^4.0.0",
    "eslint-config-prettier": "^3.1.0",
    "eslint-plugin-html": "^5.0.0",
    "eslint-plugin-only-warn": "^1.0.1",
    "eslint-plugin-prettier": "^3.0.1",
    "eslint-plugin-react": "^7.1.0",
    "eslint-plugin-vue": "^5.1.0",
    "gulp-sass": "^4.0.1",
    "mini-css-extract-plugin": "^0.5.0",
    "node-sass": "^4.5.3",
    "prettier": "^1.14.3",
    "prop-types": "^15.6.2",
    "react": "^16.5.2",
    "react-ace": "^6.1.4",
    "react-dom": "^16.5.2",
    "sass-loader": "^7.1.0",
    "style-loader": "^0.23.0",
    "styled-components": "^4.1.3",
    "webpack": "^4.19.1",
    "webpack-cli": "^3.1.0"
  }
}
